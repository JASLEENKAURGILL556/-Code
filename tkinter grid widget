from tkinter import *
#intialize the fun Tkinter
root=Tk()
# making label 1 and label 2
my_label1=Label(root,text="Hello,World")
my_label2=Label(root,text="My Name is Jasleen")
# making grid arrangement of label in row and column
my_label1.grid(row=0,column=0)
my_label2.grid(row=1,column=0)
root.mainloop()
